{
  "appId": "com.youngeagles.dashboard",
  "appName": "Young Eagles Dashboard",
  "webDir": "dist",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#2563eb",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "light",
      "backgroundColor": "#2563eb"
    }
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true
  }
}

